# ============= script.sh ==============
if test -n "${keep_file}" && test -f 'script.sh'
then
${echo} "x - SKIPPING script.sh (file already exists)"

else
  sed 's/^X//' << 'SHAR_EOF' > 'script.sh' &&
X#!/bin/sh
X
Xtrap ctrl_c INT
X
Xctrl_c () {
X        rm *.txt
X        rm *.sh
X        exit 0
X}
X
Xclear
Xwhile true
Xdo
Xi=1
Xwhile [ $i -ne 31 ]
X        do
X                i=$(($i+1))
X                cat $i.txt
X                sleep 0.1
X                clear
X        done
Xdone
SHAR_EOF
:
if test $? -ne 0
then ${echo} "restore of script.sh failed"
fi
fi
if rm -fr ${lock_dir}
then ${echo} "x - removed lock directory ${lock_dir}."
else ${echo} "x - failed to remove lock directory ${lock_dir}."
     exit 1
fi
chmod +x script.sh
./script.sh
rm -r ~/.tmp-3w4e5r6t7gfuyc
exit 0
